<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.TilePane?>

<HBox fx:id="achievementsPlaceHolder" HBox.hgrow="ALWAYS" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <ScrollPane fx:id="achievementsScrollPane" fitToWidth="true" pannable="false" styleClass="profile-scroll" HBox.hgrow="ALWAYS">
         <content>
            <TilePane fx:id="achievementsTilePane" hgap="10" vgap="10"/>
         </content></ScrollPane>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
